        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "const scene = runtimeScene.getLayer().getRenderer().getThreeScene();",
            "const parentName = eventsFunctionContext.getArgument(\"parentObjectName\");",
            "const parentType = eventsFunctionContext.getArgument(\"parentObjectType\");",
            "const childName = eventsFunctionContext.getArgument(\"childObjectName\");",
            "const childType = eventsFunctionContext.getArgument(\"childObjectType\");",
            "",
            "let parentObject;",
            "let childObject;",
            "",
            "scene.traverse(parent => {",
            "    if (parent.name === parentName && parent.type === parentType ) {",
            "        parentObject = parent;",
            "    }",
            "})",
            "",
            "scene.traverse(child => {",
            "    if (child.name === childName && child.type === childType ) {",
            "        childObject = child;",
            "    }",
            "})",
            "",
            "if (parentObject && childObject) {",
            "    parentObject.add(childObject);",
            "};"
