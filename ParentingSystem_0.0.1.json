      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "const scene = runtimeScene.getLayer(\"\").getRenderer().getThreeScene();",
            "",
            "const objectName = eventsFunctionContext.getArgument(\"objectName\");",
            "const objectType = eventsFunctionContext.getArgument(\"objectType\");",
            "const axis = eventsFunctionContext.getArgument(\"axis\");",
            "",
            "scene.traverse(node => {",
            "    if (node.name === objectName && node.type === objectType) {",
            "        const quaternion = node.quaternion;",
            "",
            "        // Convert quaternion to axis-angle representation",
            "        const angle = 2 * Math.acos(quaternion.w);",
            "        const s = Math.sqrt(1 - quaternion.w * quaternion.w);",
            "        const axisVector = new THREE.Vector3(",
            "            quaternion.x / s,",
            "            quaternion.y / s,",
            "            quaternion.z / s",
            "        );",
            "",
            "        // Extract the rotation around the desired axis",
            "        let rotationInDegrees;",
            "        if (axis === 'x') {",
            "            rotationInDegrees = THREE.MathUtils.radToDeg(angle * axisVector.x);",
            "        } else if (axis === 'y') {",
            "            rotationInDegrees = THREE.MathUtils.radToDeg(angle * axisVector.y);",
            "        } else if (axis === 'z') {",
            "            rotationInDegrees = THREE.MathUtils.radToDeg(angle * axisVector.z);",
            "        }",
            "",
            "        eventsFunctionContext.returnValue = rotationInDegrees;",
            "    }",
            "});",
            ""
